<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>jp.aeyesec.ClamdUpdate</string>
	<key>Program</key>
	<string>/bin/bash</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/malware_monitor</string>
 		<string>update</string>
	<string></string>
	</array>
	<key>RunAtLoad</key>
	<false/>
	<key>StartCalendarInterval</key>
	<array>
		<dict>
			<key>Hour</key>
			<integer>6</integer>
			<key>Minute</key>
			<integer>0</integer>
		</dict>
		<dict>
			<key>Hour</key>
			<integer>12</integer>
			<key>Minute</key>
			<integer>00</integer>
		</dict>
	</array>
	<key>StandardErrorPath</key>
	<string>/var/log/clamav/clamd.log</string>
	<key>StandardOutPath</key>
	<string>/var/log/clamav/clamd.log</string>
</dict>
</plist>